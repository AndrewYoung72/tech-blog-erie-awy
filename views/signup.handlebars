<h1>Sign up now to join!</h1>

<form class="signUp-form">
 <div class="row mb-3">
    <label for="inputName3" class="col-sm-2 col-form-label">Name</label>
    <div class="col-sm-3">
      <input type="text" class="form-control" id="name-signUp">
    </div>
  </div>
 <div class="row mb-3">
    <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-3">
      <input type="text" class="form-control" id="email-signUp">
    </div>
  </div>
  <div class="row mb-3">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
    <div class="col-sm-3">
      <input type="text" class="form-control" id="password-signUp">
    </div>
  </div>
  <button id="signUp-button" type="submit" class="btn btn-primary">Sign up</button>
</form>


<script src="./js/login.js"></script>